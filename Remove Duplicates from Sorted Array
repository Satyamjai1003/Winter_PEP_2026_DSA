class Solution {
public:
    int removeDuplicates(vector<int>&arr) {
        int i;
        int count=1;
        int n=arr.size();
        // vector<int> vt;
        for(i=0;i<n-1;i++)
        {
            if(arr[i]!=arr[i+1])
            {
                arr[count]=arr[i+1];
                count++;
            }
        }
        return count;
    }
};
